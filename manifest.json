{
  "manifest_version": 2,

  "name": "Simple notes plugin",
  "description": "Quick note taking extension",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "simplenotes.html"
  },
  "permissions": [
   "activeTab",
   "storage"
   ],

"content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "css": ["main.css"],
      "js": ["javascript/jquery-2.2.1.min.js", "simplenotes.js"]
    }
  ],

}